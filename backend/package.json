{
  "name": "backend",
  "version": "1.0.0",
  "description": "virtual store backend",
  "main": "index.js",
  "scripts": {
    "start:dev": "NODE_ENV=development && nodemon --exec babel-node --extensions \".ts\" -r tsconfig-paths/register ./src/apps/start.ts",
    "build": "babel src --extensions \".ts\" --out-dir build --copy-files",
    "typeorm:dev": "typeorm-ts-node-esm -d src/contexts/shared/infrastructure/persistence/typeorm/TypeOrmMigrations.ts",
    "m:gen": "pnpm run typeorm:dev migration:generate -p",
    "m:run": "npm run typeorm:dev migration:run",
    "test": "jest",
    "lint": "eslint --fix ./src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joganortiz/on-line-shop/backend/"
  },
  "author": "Jogan Ortiz Mu√±oz",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/node": "^7.22.19",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@types/compression": "^1.7.4",
    "@types/convict": "^6.1.5",
    "@types/cors": "^2.8.15",
    "@types/errorhandler": "^1.5.2",
    "@types/express": "^4.17.20",
    "@types/express-fileupload": "^1.4.3",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "convict": "^6.2.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "errorhandler": "^1.5.1",
    "express": "^4.18.2",
